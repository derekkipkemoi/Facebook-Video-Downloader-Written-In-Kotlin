<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="xxvidownloader.video.xxvi.downloader.videos.saver">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.WRITE_EXTERNAL_STORAGE"
        android:maxSdkVersion="28" />

    <application
        android:name="xxvidownloader.video.xxvi.downloader.videos.saver.MyApplication"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:networkSecurityConfig="@xml/network_security_config"
        tools:targetApi="n"


        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/play_store_512"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinFacebookVideoDownloader">


        <activity
            android:name="xxvidownloader.video.xxvi.downloader.videos.saver.activities.MainActivity"
            android:exported="true"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-8107055991555224~2107794569"/>



    </application>

</manifest>